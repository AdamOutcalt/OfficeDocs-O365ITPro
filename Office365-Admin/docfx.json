{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "group": "office365",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "groups": {
      "office365": {
        "dest": "o365-dest",
        "moniker_range": "=o365-worldwide || =o365-21vianet || =o365-germany"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/office365/admin/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "searchScope": [
        "Office 365 Admin"
      ],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-O365ITPro",
      "feedback_product_url": "https://office365.uservoice.com/forums/273493-office-365-admin"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-O365Admin",
    "markdownEngineName": "markdig"
  }
}
