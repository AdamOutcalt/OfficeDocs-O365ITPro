      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "group": "O365",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "groups": {
      "O365": {
          "dest":"O365-dest",
          "moniker_range": "<= O365-worldwide"
      }
  },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/office365/admin/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "searchScope": ["Office 365 Admin"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-O365ITPro",
      "feedback_product_url": "https://office365.uservoice.com/forums/273493-office-365-admin" 
    },
    "fileMetadata": {},
